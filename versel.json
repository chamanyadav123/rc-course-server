// this is vercel.json
{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "name": "server",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ]
  }
